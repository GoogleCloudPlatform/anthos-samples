{
  "displayName": "Anthos Cluster Monitoring",
  "gridLayout": {
    "columns": "2",
    "widgets": [
      {
        "title": "Node - Total, Request, Allocatable CPU cores",
        "xyChart": {
          "chartOptions": {
            "mode": "COLOR"
          },
          "dataSets": [
            {
              "minAlignmentPeriod": "60s",
              "plotType": "LINE",
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "aggregation": {
                    "crossSeriesReducer": "REDUCE_SUM",
                    "perSeriesAligner": "ALIGN_MEAN"
                  },
                  "filter": "metric.type=\"kubernetes.io/anthos/kube_pod_container_resource_requests_cpu_cores\" resource.type=\"k8s_container\"",
                  "secondaryAggregation": {}
                },
                "unitOverride": "{cpu}"
              }
            },
            {
              "minAlignmentPeriod": "60s",
              "plotType": "LINE",
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "aggregation": {
                    "crossSeriesReducer": "REDUCE_SUM",
                    "perSeriesAligner": "ALIGN_MEAN"
                  },
                  "filter": "metric.type=\"kubernetes.io/anthos/kube_node_status_allocatable_cpu_cores\" resource.type=\"k8s_container\"",
                  "secondaryAggregation": {}
                },
                "unitOverride": "{cpu}"
              }
            },
            {
              "minAlignmentPeriod": "60s",
              "plotType": "LINE",
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "aggregation": {
                    "crossSeriesReducer": "REDUCE_SUM",
                    "perSeriesAligner": "ALIGN_MEAN"
                  },
                  "filter": "metric.type=\"kubernetes.io/anthos/kube_node_status_capacity_cpu_cores\" resource.type=\"k8s_container\"",
                  "secondaryAggregation": {}
                },
                "unitOverride": "{cpu}"
              }
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "y1Axis",
            "scale": "LINEAR"
          }
        }
      },
